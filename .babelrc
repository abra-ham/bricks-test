{
	"presets": ["@babel/preset-env", "@babel/preset-react"],
	"plugins": [
		["react-css-modules", {
				"webpackHotModuleReloading": true,
				"filetypes": {
					".scss": {
						"syntax": "postcss-scss"
					}
				},
				"generateScopedName": "[sha1:hash:hex:4]"
		}],
		["@babel/plugin-proposal-class-properties"]
	]
}
